    "@types/react": "^18.0.10",
    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-react": "^1.3.2",
    "sass": "^1.52.2",
    "typescript": "^4.7.3",
    "vite": "^2.9.9",
    "vite-plugin-html": "^3.2.0"
